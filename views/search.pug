extends layouts/main
include mixins/videoBlock

block content
    .search__header
        h3 Searching by #{searchingBy}
    .search__videos
        if videos.length === 0
            h5 No Videos Found
        else
            each item in videos
                +videoBlock({
                    title: item.title,
                    views: item.description,
                    videoFile: item.fileUrl,
                    id:item.id,
                })